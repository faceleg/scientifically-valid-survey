{
  "name": "scientifically-valid-survey",
  "version": "1.0.0",
  "repository": "https://github.com/faceleg/scientifically-valid-survey",
  "description": "This is a completely valid survey tool",
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "pretest": "npm install && node_modules/.bin/bower install",
    "lintspaces": "node_modules/.bin/gulp lintspaces",
    "lint": "node_modules/.bin/eslint . && npm run lintspaces",
    "templates": "node_modules/.bin/gulp ngtemplates",
    "pretest-karma": "node_modules/.bin/bower install --allow-root && npm run templates",
    "pretest-karma-watch": "npm run templates",
    "test-karma": "node_modules/karma/bin/karma start karma.conf.js --no-auto-watch --single-run --reporters 'coverage,dots'",
    "test-karma-watch": "node_modules/karma/bin/karma start karma.conf.js --reporters 'coverage,dots'",
    "coverage-average-karma": "./node_modules/.bin/coverage-average coverage/report-summary/text-summary.txt  --limit 95",
    "test-server": "docker-compose -f server/docker-compose.yml run test /srv/www/test-server.sh",
    "test-server-watch": "docker-compose -f server/docker-compose.yml run test /srv/www/test-server-watch.sh",
    "coverage-average-server": "./node_modules/.bin/coverage-average coverage/server/text-summary.txt  --limit 50",
    "test": "npm run lint && npm run test-karma && npm run test-server",
    "code-climate": "CODECLIMATE_REPO_TOKEN=690cca4a3bf63b31ecb2864056c0be77601fb58fcea3cb13134d681963ef561e node_modules/.bin/codeclimate-test-reporter < coverage/report-lcov/lcov.info",
    "start": "pm2 start pm2.json; pm2 logs",
    "development": "pm2 start pm2-dev.json; pm2 logs",
    "precommit": "npm run lint && npm run test-karma && npm run coverage-average-karma"
  },
  "author": "Michael Robinson",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^6.2.3",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.2",
    "bower": "^1.7.2",
    "code": "^2.1.0",
    "codeclimate-test-reporter": "^0.1.1",
    "connect-livereload": "^0.5.4",
    "connect-sequelize": "^2.0.2",
    "convict": "^1.0.2",
    "coverage-average": "^1.0.4",
    "debug": "^2.2.0",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-plugin-angular": "^0.15.0",
    "express": "^4.13.3",
    "express-jwt": "^3.3.0",
    "express-session": "^1.12.1",
    "gulp": "^3.9.0",
    "gulp-angular-templatecache": "^1.8.0",
    "gulp-inject": "^3.0.0",
    "gulp-lintspaces": "^0.3.4",
    "gulp-live-server": "0.0.29",
    "gulp-ng-annotate": "^1.1.0",
    "gulp-postcss": "^6.0.1",
    "gulp-rename": "^1.2.2",
    "husky": "^0.10.2",
    "istanbul": "^0.4.1",
    "jasmine-core": "^2.4.1",
    "jsonwebtoken": "^5.5.4",
    "karma": "^0.13.18",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-notify-reporter": "^0.1.1",
    "karma-phantomjs-launcher": "^0.2.3",
    "lost": "^6.6.3",
    "mocha": "^2.3.4",
    "mysql": "^2.10.0",
    "phantomjs": "^1.9.19",
    "postcss": "^5.0.14",
    "postcss-color-function": "^2.0.0",
    "postcss-extend": "^1.0.1",
    "postcss-import": "^7.1.3",
    "postcss-nested": "^1.0.0",
    "postcss-responsive-type": "^0.3.2",
    "postcss-simple-vars": "^1.2.0",
    "ramda": "^0.19.1",
    "run-sequence": "^1.1.5",
    "sequelize": "^3.15.1",
    "sequelize-cli": "^2.2.1",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0",
    "test-plan": "^0.1.1",
    "yargs": "^3.31.0"
  }
}
